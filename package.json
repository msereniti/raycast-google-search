{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "google-search",
  "title": "Google Search",
  "description": "A Raycast extension that allows you to do googling without opening Google's UI.",
  "icon": "google-icon.png",
  "author": "phytonmk",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search in Google",
      "description": "Do googling without opening the browser",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.53.2",
    "@raycast/utils": "^1.4.8",
    "googlethis": "1.7.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@raycast/eslint-plugin": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.2.12",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "react": "18.2.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "overrides": {
    "axios": "1.4.0"
  }
}